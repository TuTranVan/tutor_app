<% provide(:title, "Môn học") %>
<div class="row">
  <div class="col-md-2">
    <%= render "layouts/nav" %>
  </div>
  <div class="col-md-10 content-admin">
    <div class="text-center">
      <h2><b>Danh sách môn học</b></h2>
    </div>
    <p><%= link_to "THÊM MỚI", new_admin_subject_path, class: "button-background" %></p>
    <br>
    <table class="table table-striped table-bordered table-list text-center">
      <thead>
        <tr>
          <th class="text-center"><em class="fa fa-cog"></em></th>
          <th class="text-center">Tên môn học</th>
        </tr>
      </thead>
      <tbody>
        <%= render @subjects %>
      </tbody>
    </table>
  </div>
</div>

